{
	"$schema": "https://json-schema.org/draft-07/schema",
	"$id": "http://platform.selfkey.org/schema/document/audio.json",
	"type": "object",
	"format": "file",
	"title": "Audio",
	"allOf": [
		{
			"$ref": "http://platform.selfkey.org/schema/document/document.json"
		},
		{
			"properties": {
				"mimeType": {
					"type": "string",
					"enum": ["audio/mp3", "audio/mp4", "audio/ogg", "audio/x-wav"]
				},
				"size": { "type": "integer", "max": 50000000 }
			}
		}
	]
}
