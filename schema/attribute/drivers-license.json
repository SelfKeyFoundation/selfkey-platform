{
	"$id": "http://platform.selfkey.org/schema/attribute/drivers-license.json",
	"$schema": "http://platform.selfkey.org/schema/identity-attribute.json",
	"identityAttribute": true,
	"identityAttributeRepository": "http://platform.selfkey.org/repository.json",
	"title": "Drivers License",
	"description": "A copy of an individuals drivers license.",
	"type": "object",
	"properties": {
		"front": {
			"type": "object",
			"title": "Front image",
			"properties": {
				"$ref": "http://platform.selfkey.org/schema/document/image.json"
			},
			"required": ["image"]
		},
		"back": {
			"type": "object",
			"title": "Back image",
			"properties": {
				"$ref": "http://platform.selfkey.org/schema/document/image.json"
			},
			"required": ["image"]
		}
	},
	"required": ["front", "back"]
}
