{
	"title": "Not in sanction lists credential",
	"$id": "http://platform.selfkey.org/schema/credentials/not-in-sanction-lists.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"allOf": [
		{ "$ref": "http://platform.selfkey.org/schema/verifiable-credential.json" },
		{
			"type": "object",
			"properties": {
				"name": {
					"type": "string",
					"const": "Not in sanction lists credential"
				},
				"notInSanctionLists": {
					"type": "boolean",
					"const": true
				}
			},
			"required": ["notInSanctionLists"]
		}
	]
}
