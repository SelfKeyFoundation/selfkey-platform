{
	"title": "KeyFi Eligibility Credential",
	"$id": "http://platform.selfkey.org/schema/credentials/keyfi.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"allOf": [
		{ "$ref": "http://platform.selfkey.org/schema/verifiable-credential.json" },
		{
			"type": "object",
			"properties": {
				"name": {
					"type": "string",
					"const": "KeyFi Eligibility Credential"
				},
				"keyfiEligibility": {
					"type": "boolean",
					"const": true
				}
			},
			"required": ["keyfiEligibility"]
		}
	]
}
