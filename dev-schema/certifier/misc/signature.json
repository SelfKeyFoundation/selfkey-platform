"$schema": "http://json-schema.org/draft-07/schema",
"$id": "http://platform.selfkey.org/schema/certifier/signature.json",
"title": "Signature",
"description": "The information contiained in an electronic signature.",
"type": "object",
"properties": {
	"version":  {
		"type": "string"
	},
	"signatureAlgorithm":  {
		"type": "string"
	},
	"subject": {
		"type": "string"
	},
	"issuer": {
		"type": "string"
	},
	"serialNumber": {
		"type": "string"
	},
	"validityStarts": {
		"$ref": "http://platform.selfkey.org/schema/misc/issue-date.json"
	},
	"validityEnds": {
		"$ref": "http://platform.selfkey.org/schema/misc/expiry-date.json"
	},
	"keyUsage": {
		"type": "string"
	},
	"publicKey": {
		"type": "string"
	},
	"SHA1DigestOfPublicKey": {
		"type": "string"
	},
	"x509Data": {
		"type": "string"
	},
	"SHA1Digest": {
		"type": "string"
	},
	"MD5Digest": {
		"type": "string"
	}
}